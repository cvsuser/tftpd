CC	Makefile	/^CC=gcc$/;"	m
CFLAGS	Makefile	/^CFLAGS=-Wall -std=c99 -ggdb $/;"	m
HEADERS	Makefile	/^HEADERS=tftpd.h$/;"	m
LDFLAGS	Makefile	/^LDFLAGS=-ggdb$/;"	m
LDLIBS	Makefile	/^LDLIBS=-lreadline$/;"	m
OBJECTS	Makefile	/^OBJECTS=$(SOURCES:.c=.o)$/;"	m
SOURCES	Makefile	/^SOURCES=tftpd.c$/;"	m
TARGET	Makefile	/^TARGET=tftpd$/;"	m
